#!/bin/bash

# -- jojo --
# description: wget github docker.pyjojo docker.location
# param:project 
# -- jojo --

echo "jojo_return_value project=$PROJECT"
wget   https\://raw.githubusercontent.com/GiftLee/webapp/master/docker.pyjojo -O /srv/pyjojo/docker.pyjojo
wget   https\://raw.githubusercontent.com/GiftLee/webapp/master/location.conf -O /srv/pyjojo/location.conf
sh /srv/pyjojo/$PROJECT.pyjojo
exit 0
